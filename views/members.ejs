<% if (!session || !session.authenticated) { %>
    <% res.redirect('/'); %>
<% } else { %>
    <!DOCTYPE html>
    <html>
        <head>
            <title>Members</title>
        </head>
        <body>
            <h1>Hello, <%= username %></h1>
            <img src="<%= image %>" />
            <br><br>
            <form action="/logout" method="post">
                <button type="submit" onclick="window.location.href = '/'">Sign Out</button>
            </form>
        </body>
    </html>
<% } %>

  
